{
  "type": "AdaptiveCard",
  "body": [
    {
      "type": "ColumnSet",
      "columns": [
        {
          "type": "Column",
          "items": [
            {
              "type": "TextBlock",
              "weight": "bolder",
              "text": "Caroline Rich",
              "wrap": true,
              "size": "large",
              "style": "heading"
            }
          ],
          "width": "stretch"
        },
        {
          "type": "Column",
          "width": "auto",
          "items": [
            {
              "type": "Image",
              "url": "data:image/png;base64,",
              "spacing": "none",
              "horizontalAlignment": "center",
              "width": "11px",
              "height": "11px",
              "selectAction": {
                "type": "Action.ToggleVisibility"
              }
            }
          ],
          "horizontalAlignment": "Center"
        }
      ]
    },
    {
      "type": "ColumnSet",
      "columns": [
        {
          "type": "Column",
          "width": "auto",
          "items": [
            {
              "type": "Image",
              "urlString": "data:image/svg+xml;base64,",
              "url": "data:image/svg+xml;base64,",
              "spacing": "None"
            }
          ]
        },
        {
          "type": "Column",
          "width": "stretch",
          "items": [
            {
              "type": "TextBlock",
              "spacing": "None",
              "text": "${formatDateTime('2023-08-04T00:00:00Z', 'MMMM d, yyyy')}",
              "isSubtle": true,
              "wrap": true,
              "weight": "Bolder",
              "fontType": "Default",
              "style": "default"
            }
          ]
        }
      ]
    },
    {
      "type": "TextBlock",
      "text": "The customer reported that their washer has a foul smell.",
      "wrap": true
    }
  ],
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.6",
  "rtl": false
}
